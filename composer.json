{
    "name": "xmo/mine",
    "type": "library",
    "license": "Apache-2.0",
    "keywords": [
        "php",
        "mine",
        "hyperf"
    ],
    "description": "mine hyperf kernel",
    "autoload": {
        "psr-4": {
            "Mine\\": "src/"
        },
        "files": [
            "src/Helper/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "MineTest\\": "tests"
        }
    },
    "require": {
        "php": ">=8.0",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-redis": "*",
        "ext-swoole": ">=4.5",
        "doctrine/dbal": "^3.1",
        "easyswoole/verifycode": "3.x",
        "hyperf/amqp": "^2.2.0",
        "hyperf/cache": "^2.2.0",
        "hyperf/command": "^2.2.0",
        "hyperf/config": "^2.2.0",
        "hyperf/crontab": "^2.2.0",
        "hyperf/database": "^2.2.0",
        "hyperf/db-connection": "^2.2.0",
        "hyperf/filesystem": "^2.2.0",
        "hyperf/framework": "^2.2.0",
        "hyperf/guzzle": "^2.2.0",
        "hyperf/http-server": "^2.2.0",
        "hyperf/logger": "^2.2.0",
        "hyperf/memory": "^2.2.0",
        "hyperf/model-cache": "^2.2.0",
        "hyperf/paginator": "^2.2.0",
        "hyperf/pool": "^2.2.0",
        "hyperf/process": "^2.2.0",
        "hyperf/redis": "^2.2.0",
        "hyperf/translation": "^2.2.0",
        "hyperf/validation": "^2.2.0",
        "hyperf/websocket-server": "^2.2.0",
        "overtrue/flysystem-cos": "^3.0",
        "overtrue/flysystem-qiniu": "^1.0",
        "phpoffice/phpspreadsheet": "^1.18",
        "symfony/property-access": "^6.0",
        "tangwei/apidocs": "^1.0",
        "xmo/jwt-auth": "0.2.*",
        "xxtime/flysystem-aliyun-oss": "^1.5",
        "zoujingli/ip2region": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "hyperf/devtool": "^2.2.0",
        "hyperf/ide-helper": "^2.2.0",
        "hyperf/testing": "^2.2.0",
        "hyperf/watcher": "^2.2",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^0.12",
        "qiutuleng/hyperf-dump-server": "^1.2",
        "swoole/ide-helper": "^4.5"
    },
    "suggest": {
        "ext-openssl": "Required to use HTTPS.",
        "ext-json": "Required to use JSON.",
        "ext-pdo": "Required to use MySQL Client.",
        "ext-pdo_mysql": "Required to use MySQL Client.",
        "ext-redis": "Required to use Redis Client."
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "Mine\\ConfigProvider"
        }
    }
}
